package com.ja.webproject.vo;

public class HE_LostDogBoard_ImageBoardVo {
	
	private int   image_no;
    private int lostdogboard_no;
    private String image_url;
    private String image_ori;
	public HE_LostDogBoard_ImageBoardVo() {
		super();
	}
	public HE_LostDogBoard_ImageBoardVo(int image_no, int lostdogboard_no, String image_url, String image_ori) {
		super();
		this.image_no = image_no;
		this.lostdogboard_no = lostdogboard_no;
		this.image_url = image_url;
		this.image_ori = image_ori;
	}
	public int getImage_no() {
		return image_no;
	}
	public void setImage_no(int image_no) {
		this.image_no = image_no;
	}
	public int getLostdogboard_no() {
		return lostdogboard_no;
	}
	public void setLostdogboard_no(int lostdogboard_no) {
		this.lostdogboard_no = lostdogboard_no;
	}
	public String getImage_url() {
		return image_url;
	}
	public void setImage_url(String image_url) {
		this.image_url = image_url;
	}
	public String getImage_ori() {
		return image_ori;
	}
	public void setImage_ori(String image_ori) {
		this.image_ori = image_ori;
	}
		
	
	
    

}
